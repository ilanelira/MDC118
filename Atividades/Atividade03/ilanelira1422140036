}





2)
#include <stdio.h>

int main()

{

    int t1, t2, h;

    t1 = 0;

    t2 = 1;

    for(int i = 0; i < 10; i++)

   {

         h = t1 + t2;

         t1 = t2;

         t2 = h;

         printf("%d", h);

 }    
 
}


3)

#include <stdio.h>
int main ()
{
int fat, n;
printf("Insira por gentileza um valor no qual voce deseja calcular seu fatorial: ");
scanf("%d", &n);
for(fat = 1; n > 1; n = n - 1)
fat = fat * n;
printf("\n   O valor fica: %d", fat);

  return 0;
}
